package oops;

public class MultipleConstuctors {
	
	String name;
	int age;
	public static void main(String args[]){
		
				
		
		
	}
	
	MultipleConstuctors(String name){
		this.name=name;
	}
	
	MultipleConstuctors(String name, int age){
		this.name=name;
		this.age=age;
	}
	
	public void display(){
		System.out.println("Name:"+name+"\tage"+age);
		
	}

}
