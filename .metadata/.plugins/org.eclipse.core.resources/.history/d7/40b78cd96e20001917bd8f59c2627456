import java.util.*;

public class Sample {

	public static void main(String args[]) {
		Sample1 s= new Sample1(100,"Praveen");
		Sample1 s1=new Sample1(s);
		System.out.println(s.name);
	}
}


class Sample1 {
	 int id;
	 String name;
	 
	 Sample1(int id, String name) {
		 this.id=id;
		 this.name=name;
	 } 
	 
	 Sample1(Sample1 s) {
		 id=s.id+ 100;
		 name=s.name+" Pavan";
	 }
	 
	 public String toString() {
		 return "id:"+id+" name:"+name;
	 }
}









































